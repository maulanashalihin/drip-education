      <title>Cheatsheet</title>

      <style>
         body {
            color: black;
            margin: 0;
            padding: 0;
         }
         main {
            max-width: 1000px;
            min-width: 1000px;
            width: 1000px;
            margin: 30px auto 50px;
         }
         .bg-row {
            display: flex;
            align-items: center;
            flex-wrap: wrap;
         }
         .bg-row div {
            width: 100px;
            height: 40px;
            display: flex;
            align-items: center;
            justify-content: center;
            margin-bottom: 30px;
            margin-right: 10px;
            position: relative;
            box-shadow: 0px 0px 10px 0px rgba(14, 122, 204, 0.3);
         }
         .bg-row div span {
            position: absolute;
            bottom: -25px;
            left: 0;
            width: 100%;
            display: block;
            text-align: center;
         }
         .bg-row div::before {
            content: "";
            position: relative;
            font-size: 10px;
         }
         .sizes div {
            height: 20px;
            background-color: rgb(9, 161, 216);
            display: flex;
            align-items: center;
            margin-bottom: 3px;
            white-space: nowrap;
            padding-left: 5px;
         }
         .w-screen {
            position: relative;
            left: 50%;
            transform: translate(-50%);
         }
         .borders {
            display: flex;
            flex-wrap: wrap;
         }
         .borders-row {
            display: flex;
            justify-content: space-between;
            width: 100%;
         }
         .borders-row div {
            width: 190px;
            background-color: rgb(9, 161, 216);
            justify-content: center;
            display: flex;
            align-items: center;
            height: 50px;
            border-color: rgb(6, 59, 100);
         }

         .rounds div {
            display: flex;
            justify-content: space-between;
            align-items: center;
         }
         .rounds div div {
            width: 190px;
            height: 30px;
            background-color: rgb(9, 161, 216);
            margin-bottom: 5px;
            justify-content: center;
         }

         .border-types div {
            width: 190px;
            border-color: rgb(9, 161, 216);
            display: flex;
            align-items: center;
            justify-content: center;
            height: 50px;
         }

         .shadows {
            display: flex;
         }
         .shadows div {
            width: 190px;
            height: 50px;
            margin-right: 10px;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 14px;
         }

         .link {
            padding: 10px 20px;
            background-color: rgb(113, 222, 255);
            transition:
               background-color 0.25s ease,
               color 0.25s ease;
            margin-right: 10px;
            display: flex;
            align-items: center;
            justify-content: center;
         }
         .link svg {
            width: 15px;
            height: 15px;
            margin-left: 5px;
            transition: fill 0.25s ease;
         }
         .link:hover {
            background-color: rgb(25, 131, 163);
            color: white;
         }
         .link:hover svg {
            fill: white;
         }
      </style>
